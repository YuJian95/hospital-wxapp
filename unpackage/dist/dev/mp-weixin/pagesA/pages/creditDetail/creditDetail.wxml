<view class="weui-tab body"><view class="weui-navbar"><block wx:for="{{tabs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['weui-navbar__item',activeIndex==index?'weui-bar__item_on nav-bottom-border':'']}}" id="{{index}}" data-event-opts="{{[['tap',[['tabClick',['$event']]]]]}}" bindtap="__e"><view class="weui-navbar__title">{{item}}</view></view></block></block></view><view class="weui-tab__panel"><view class="weui-tab__content" hidden="{{activeIndex!=0}}"><credit-page class="vue-ref" hidden="{{!(activeIndex==0)}}" vue-id="1" data-ref="creditPage" bind:__l="__l"></credit-page></view><view class="weui-tab__content" hidden="{{activeIndex!=1}}"><history-credit class="vue-ref" hidden="{{!(!isCreditGood)}}" vue-id="2" data-ref="historyCredit" bind:__l="__l"></history-credit></view></view><image class="icon-up-position" src="/static/appointment/icon-up.png" hidden="{{activeIndex!=1}}" data-event-opts="{{[['tap',[['showModal']]]]}}" bindtap="__e"></image><view class="{{['cu-modal bottom-modal',isShowModal?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white"><view data-event-opts="{{[['tap',[['changeCardId']]]]}}" class="action text-green" bindtap="__e">确定</view><view data-event-opts="{{[['tap',[['hideModal']]]]}}" class="action text-blue" bindtap="__e">取消</view></view><view class="bottom-hospital-box"><block wx:for="{{cardList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="hospital-box"><view data-event-opts="{{[['tap',[['selectCardId',['$0'],[[['cardList','id',item.id,'id']]]]]]]}}" class="{{['hospital',modalVisited==item.id?'visited-background':'invisited-background']}}" bindtap="__e"><text class="text">{{''+item.name+''}}</text></view></view></block></view></view></view></view>