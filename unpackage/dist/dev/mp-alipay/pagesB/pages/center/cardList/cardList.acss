    border-left: 6px solid #7EC0EE;
}
.blue-column-border-box .blue-text {
    width: 80%;
    font-size: 18px;
    font-weight: bold;
    color: #7EC0EE;
}
.main {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
