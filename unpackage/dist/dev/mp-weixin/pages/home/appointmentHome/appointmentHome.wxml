<view><block wx:if="{{insureVisited==0}}"><from-hosipital vue-id="1" bind:__l="__l"></from-hosipital></block><block wx:if="{{insureVisited==1}}"><from-doctor vue-id="2" bind:__l="__l"></from-doctor></block><block wx:if="{{insureVisited==2}}"><from-date vue-id="3" bind:__l="__l"></from-date></block><block wx:if="{{insureVisited==3}}"><department vue-id="4" bind:__l="__l"></department></block><image class="icon-up-position" src="/static/appointment/icon-up.png" data-event-opts="{{[['tap',[['showModal']]]]}}" bindtap="__e"></image><view class="{{['cu-modal bottom-modal',isShowModal?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white"><view data-event-opts="{{[['tap',[['changeFrom',['$event']]]]]}}" class="action text-green" bindtap="__e">确定</view><view data-event-opts="{{[['tap',[['hideModal']]]]}}" class="action text-blue" bindtap="__e">取消</view></view><view class="change-appoint-form"><block wx:for="{{modalData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectFrom',[index]]]]]}}" class="appoint-inbox" bindtap="__e"><block wx:if="{{modalVisited==index}}"><view data-name="{{index}}"><view class="round-icon-box visited-round"><image class="icon" src="{{imgUrl+item.visited}}"></image></view><text class="visited-text">{{item.text}}</text></view></block><block wx:else><view data-name="{{index}}"><view class="round-icon-box invisited-round"><image class="icon" src="{{imgUrl+item.invisited}}"></image></view><text class="invisited-text">{{item.text}}</text></view></block></view></block></view></view></view></view>