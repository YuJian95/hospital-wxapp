.page-backgroud {
  width: 100%;
  height: 100%;
  background: #F2F2F2;
}
.visited-color {
  color: #7EC0EE !important;
}
.cut-long-text {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.tips-box {
  width: 100%;
  height: 100rpx;
  border: 1px solid #F0F0F0;
  background: #FFFEF5;
}
.tips-box .tips-inbox {
    width: 80%;
    height: 100%;
    margin: 0 auto;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.tips-box .tips-inbox .icon {
      width: 60rpx;
      height: 60rpx;
      margin-right: 30rpx;
}
.tips-box .tips-inbox .tips-text {
      font-size: 17px;
      font-weight: 500;
      color: #A6A6A6;
}
.gray-border-box {
  width: 90%;
  height: 100%;
  margin: 0 auto;
  border: 1px solid #BBBBBB;
  border-radius: 8px;
  padding: 10rpx;
  background: #FFFFFF;
}
.blue-column-border-box {
  width: 90%;
  height: 90rpx;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 10rpx;
}
.blue-column-border-box .blue-border {
    width: 5%;
    height: 50rpx;
    border-left: 6px solid #7EC0EE;
}
.blue-column-border-box .blue-text {
    width: 80%;
    font-size: 18px;
    font-weight: bold;
    color: #7EC0EE;
}
page {
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
}
.weui-tab__content {
  text-align: center;
}
.body {
  width: 100%;
  height: 100%;
  background: #FFFFFF;
}
.nav-bottom-border {
  border-bottom: 2px solid #7EC0EE;
}
.icon-up-position {
  width: 70rpx;
  height: 70rpx;
  position: fixed;
  bottom: 0px;
  right: 0rpx;
}
.bottom-hospital-box {
  width: 100%;
  height: 100%;
  background: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.bottom-hospital-box .hospital-box {
    width: 33%;
    height: 80rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    margin-bottom: 20rpx;
}
.bottom-hospital-box .hospital-box .hospital {
      width: 85%;
      height: 85%;
      margin: 0 auto;
      border-radius: 10px;
      text-align: center;
      display: table;
}
.bottom-hospital-box .hospital-box .hospital .text {
        width: 100%;
        height: 100%;
        font-size: 16px;
        font-weight: 500;
        color: #FFFFFF;
        display: table-cell;
        vertical-align: middle;
}
.bottom-hospital-box .hospital-box .visited-background {
      background: #7EC0EE;
}
.bottom-hospital-box .hospital-box .invisited-background {
      background: #D1D1D1;
}

