.page-backgroud, page {
  width: 100%;
  height: 100%;
  background: #F2F2F2;
}
.visited-color {
  color: #7EC0EE !important;
}
.cut-long-text {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.tips-box {
  width: 100%;
  height: 100rpx;
  border: 1px solid #F0F0F0;
  background: #FFFEF5;
}
.tips-box .tips-inbox {
    width: 80%;
    height: 100%;
    margin: 0 auto;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.tips-box .tips-inbox .icon {
      width: 60rpx;
      height: 60rpx;
      margin-right: 30rpx;
}
.tips-box .tips-inbox .tips-text {
      font-size: 17px;
      font-weight: 500;
      color: #A6A6A6;
}
.gray-border-box {
  width: 90%;
  height: 100%;
  margin: 0 auto;
  border: 1px solid #BBBBBB;
  border-radius: 8px;
  padding: 10rpx;
  background: #FFFFFF;
}
.blue-column-border-box {
  width: 90%;
  height: 90rpx;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
